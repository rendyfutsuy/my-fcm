{
    "name": "rendyfutsuy/my-fcm",
    "description": "A simple package that help you send a Firebase notification with your Laravel applications.",
    "require-dev": {},
    "license": "MIT",
    "authors": [
        {
            "name": "rendyfutsuy",
            "email": "rendyx942000@gmail.com"
        }
    ],
    "autoload" : {
        "psr-4" : {
            "Angga\\Fcm\\" : "src/"
        },
        "files" : [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": "^7.0",
        "illuminate/support": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Angga\\Fcm\\FcmServiceProvider"
            ],
            "aliases" : {
                "Fcm": "Angga\\Fcm\\FcmFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "dev-package": {
            "type" : "path",
            "url": "C:\\laragon\\www\\package-laravel-fcm",
            "options": {
                "symlink": true
            }
        }
    }
}
